  "description": "MessagePack RPC implementation for Node",
  "main": "./dist/index.js",
  "types": "./dist/index.d.js",
  "scripts": {
    "pretest": "npm run build",
    "test": "jest",
    "test:watch": "jest -w",
    "build": "tsc",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint --ext .ts src",
