  "devDependencies": {
    "@types/jest": "24.0.19",
    "@types/node": "12.12.3",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "eslint": "6.3.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "ts-jest": "24.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.7.2"
